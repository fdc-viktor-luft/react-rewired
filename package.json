{
  "name": "wired-react",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "start": "react-scripts start",
    "docs": "react-scripts build && rm -rf docs && mv build docs",
    "dist": "bin/dist.sh",
    "test": "react-scripts test --env=jsdom",
    "flow": "flow --quiet",
    "eslint": "eslint --cache --cache-location 'misc/.eslintcache' src/",
    "prettier": "prettier --config .prettierrc --write 'src/**/*.js'",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "peerDependencies": {
    "react": ">= 16.2.0"
  },
  "dependencies": {},
  "devDependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-scripts": "1.1.4",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "flow-bin": "^0.73.0",
    "prettier": "^1.13.4"
  }
}
